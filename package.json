{
  "name": "react-native-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "run:ios": "react-native run-ios",
    "run:android": "react-native run-android",
    "pod:install": "cd ios && pod install",
    "test": "jest",
    "lint": "tslint --project tsconfig.json --config tslint.json 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.1",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-localization": "^2.1.5",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-sqlite-storage": "^3.3.11",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.16",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.60.2",
    "@types/react-native-sqlite-storage": "^3.3.1",
    "@types/react-native-vector-icons": "^6.4.1",
    "@types/react-test-renderer": "^16.8.3",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "16.8.6",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "jest": {
    "preset": "react-native"
  }
}
